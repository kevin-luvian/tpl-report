<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/view_navigation"
    app:startDestination="@id/editCartList">
    <fragment
        android:id="@+id/editCartList"
        android:name="id.ac.ui.cs.mobileprogramming.kevinlh.cartminder2.addeditscreen.EditCartList"
        android:label="fragment_edit_cart_list"
        tools:layout="@layout/fragment_edit_cart_list">
        <action
            android:id="@+id/action_editCartList_to_addEditCart"
            app:destination="@id/addEditCart"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/idle"
            app:popEnterAnim="@anim/idle"
            app:popExitAnim="@anim/slide_out_left" />
    </fragment>
    <fragment
        android:id="@+id/addEditCart"
        android:name="id.ac.ui.cs.mobileprogramming.kevinlh.cartminder2.addeditscreen.AddEditCart"
        android:label="fragment_add_edit_cart"
        tools:layout="@layout/fragment_add_edit_cart">
        <action
            android:id="@+id/action_addEditCart_to_addEditItem"
            app:destination="@id/addEditItem"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/idle"
            app:popEnterAnim="@anim/idle"
            app:popExitAnim="@anim/slide_out_left" />
        <argument
            android:name="cart"
            android:defaultValue="@null"
            app:argType="id.ac.ui.cs.mobileprogramming.kevinlh.cartminder2.model.Cart"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/addEditItem"
        android:name="id.ac.ui.cs.mobileprogramming.kevinlh.cartminder2.addeditscreen.AddEditItem"
        android:label="fragment_add_edit_item"
        tools:layout="@layout/fragment_add_edit_item">
        <argument
            android:name="IWDPos"
            android:defaultValue="-1"
            app:argType="integer" />
        <action
            android:id="@+id/action_addEditItem_to_addEditDetail"
            app:destination="@id/addEditDetail"
            app:enterAnim="@anim/slide_in_left"
            app:exitAnim="@anim/idle"
            app:popEnterAnim="@anim/idle"
            app:popExitAnim="@anim/slide_out_left" />
    </fragment>
    <fragment
        android:id="@+id/addEditDetail"
        android:name="id.ac.ui.cs.mobileprogramming.kevinlh.cartminder2.addeditscreen.AddEditDetail"
        android:label="fragment_add_edit_detail"
        tools:layout="@layout/fragment_add_edit_detail">
        <argument
            android:name="IWDPos"
            android:defaultValue="-1"
            app:argType="integer" />
        <action
            android:id="@+id/action_addEditDetail_to_bottomInputDialog"
            app:destination="@id/bottomInputDialog" />
    </fragment>
    <dialog
        android:id="@+id/bottomInputDialog"
        android:name="id.ac.ui.cs.mobileprogramming.kevinlh.cartminder2.addeditscreen.BottomInputDialog"
        android:label="fragment_bottom_input_dialog"
        tools:layout="@layout/fragment_bottom_input_dialog">
        <argument
            android:name="IWDPos"
            android:defaultValue="-1"
            app:argType="integer" />
    </dialog>
</navigation>